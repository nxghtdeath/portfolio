  function show_window(id, settings) {
    settings = settings || {};
    id = '#' + id;
    if ($(id).attr('class') == 'information_window') {
      $(id + '[data-processed != "yes"]').each(function() {
        buttons = $(id).data('buttons').split(',');
        $(id).html($(id).html() + "<div class=buttons></div>");
        $('#message_window_buttons input[data-button]').each(function() {
          if ($.inArray($(this).data('button'), buttons) != -1) 
            $(this).clone(true).appendTo(id + ' .buttons');
        });
        $(id + ' .caption').html($(id + ' .caption').html() + '<div class="close" data-button="close">X</div>');
        $(id + ' div[data-button], ' + id + ' input[data-button]').attr('onclick', 'window.' + $(id).data('result-function') + '({button: $(this).attr("data-button")})');
        $(id).attr('data-processed', 'yes');
      });
      $.fancybox({
        maxWidth: 500,
        wrapCSS: "information_window_shadow",
        helpers : {
          overlay: {
    			  locked: false,
            css : {
              'background' : 'none'
            }
    			}
        },
        tpl: {
  				closeBtn: ''
  			},
        'href' : id
    	});  
    }  
  } 
</script>

