  var want_download_sending = false;
  function want_download(game_id) {  
    if (!want_download_sending) {
      want_download_sending = true;
    	$.post('/games/want_download/', {
    		game_id: game_id
      }, function(data) {
        want_download_sending = false;
        eval(data);
    	});	
    }
  }
  
</script>

